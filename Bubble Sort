//  selection sort
let arr = [
  1,  2,  3,  34, 44,
  5, 33, 22, 121,  4
]
  
   for(let i = 0; i<arr.length;i++){

        for(let j = 0;j<arr.length-i-1;j++){
              
               if(arr[j]>arr[j+1]){
                   swap(arr,j,j+1)
               }
        }
   }
   
   
   
   function swap(arr,i,j){
       let temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp
   }
console.log(arr)
